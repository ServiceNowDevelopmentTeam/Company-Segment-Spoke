    <sys_script_fix action="INSERT_OR_UPDATE">
        <before>false</before>
        <description/>
        <name>Sector to Segment</name>
        <record_for_rollback>false</record_for_rollback>
        <script><![CDATA[//Enable sectors field
var dictionaryGr = new GlideRecord('sys_dictionary');
dictionaryGr.get('6b0a342f1b139c10e7004229bc4bcb3f');//Sectors field
dictionaryGr.setValue('active','false');
dictionaryGr.update();

//If segment is blank and sectors is not then set segment to be equal to sectors
var companyGr = new GlideRecord('core_company');
companyGr.query();
while(companyGr.next()){
	if(gs.nil(companyGr.getValue('u_segment')) && !gs.nil(companyGr.getValue('u_sectors'))){
		companyGr.setValue('u_segment',companyGr.u_sectors);
		companyGr.update();
	}
}

//Disable sectors field
dictionaryGr = new GlideRecord('sys_dictionary');
dictionaryGr.get('6b0a342f1b139c10e7004229bc4bcb3f');//Sectors field
dictionaryGr.setValue('active','false');
dictionaryGr.update();]]></script>
